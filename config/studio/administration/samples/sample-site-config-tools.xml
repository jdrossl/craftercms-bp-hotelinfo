                    </icon>
                </control>
                <control>
                    <name>page-nav-order</name>
                    <icon>
                        <class>fa-sort</class>
                        <styles>
                            <color>#7e9dbb</color>
                        </styles>
                    </icon>
                </control>
                <control>
                    <name>file-name</name>
                    <icon>
                        <class>fa-file-o</class>
                        <styles>
                            <color>#7e9dbb</color>
                        </styles>
                    </icon>
                </control>
                <control>
                    <name>auto-filename</name>
                    <icon>
                        <class>fa-file-o</class>
                        <stackedclass>fa-magic</stackedclass>
                        <styles>
