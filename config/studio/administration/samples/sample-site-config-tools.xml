<config>
	<tools>
		<tool>
			<name>content-types</name>
			<label>Content Types</label>
			<icon>
                <styles>	<!-- Change default icon styles - using css rules -->
                    <color>#409a00</color>
                    <font-size>16px</font-size>
                    <font-width>bold</font-width>
                </styles>
            </icon>
            <formSection>
                <icon>
                    <class>fa-object-group</class>
                    <styles>
                        <color>#7e9dbb</color>
                    </styles>
                </icon>
            </formSection>
            <repeatSection>
                <icon>
                    <class>fa-repeat</class>
                    <styles>
                        <color>#7e9dbb</color>
                    </styles>
                </icon>
            </repeatSection>
            <controls>
                <control>
                    <name>input</name>
                    <icon>
                        <class>fa-pencil-square-o</class>
                        <styles>
                            <color>#7e9dbb</color>
                        </styles>
                    </icon>
                </control>
                <control>
                    <name>textarea</name>
                    <icon>
                        <class>fa-paragraph</class>
                        <styles>
                            <color>#7e9dbb</color>
                        </styles>
                    </icon>
                </control>
                <control>
                    <name>rte</name>
                    <icon>
                        <class>fa-code</class>
                        <styles>
                            <color>#7e9dbb</color>
                        </styles>
                    </icon>
                </control>
                <control>
                    <name>dropdown</name>
                    <icon>
                        <class>fa-sort-desc</class>
                        <styles>
                            <color>#7e9dbb</color>
                        </styles>
                    </icon>
                </control>
                <control>
                    <name>date-time</name>
                    <icon>
                        <class>fa-calendar</class>
                        <stackedclass>fa-clock-o</stackedclass>
                        <styles>
                            <color>#7e9dbb</color>
                        </styles>
                    </icon>
                </control>
                <control>
                    <name>checkbox</name>
                    <icon>
                        <class>fa-check-square-o</class>
                        <styles>
                            <color>#7e9dbb</color>
                        </styles>
                    </icon>
                </control>
                <control>
                    <name>checkbox-group</name>
                    <icon>
                        <class>fa-check-square-o</class>
                        <styles>
                            <color>#7e9dbb</color>
                        </styles>
                    </icon>
                </control>
                <control>
                    <name>node-selector</name>
                    <icon>
                        <class>fa-crosshairs</class>
                        <styles>
                            <color>#7e9dbb</color>
                        </styles>
                    </icon>
                </control>
                <control>
                    <name>image-picker</name>
                    <icon>
                        <class>fa-picture-o</class>
                        <styles>
                            <color>#7e9dbb</color>
